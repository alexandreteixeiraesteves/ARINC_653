(*--------------------------------------------------*)
(* Some int-specific functions                      *)
(*--------------------------------------------------*)
fun int_is_odd (i:int) returns (o:bool)

(*--------------------------------------------------*)
(* Convert int to float (needed when reading data). *)
(*--------------------------------------------------*)
fun int2float(i:int) returns (o:float)
fun float2int(i:float) returns (o:int)

(*--------------------------------------------------*)
(* External functions on float arguments.           *)
(*--------------------------------------------------*)
fun round(i:float) returns (o:float)
fun asin(i:float) returns (o:float)
fun acos(i:float) returns (o:float)
fun atan2(im:float;re:float) returns (ang:float)
fun sqrt(i:float) returns (o:float)
fun sin(i:float) returns (o:float)
fun cos(i:float) returns (o:float)
fun pow(i:float;j:float) returns (o:float)
